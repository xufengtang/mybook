<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="cn.ewb.cq.domain">

	<class name="LisenceInfo" table="tc_lisence_info">
		<id name="id" length="50">
			<generator class="uuid.hex"/>
		</id>
		<property name="lisenceCode" length="15" column="LISENCE_CODE"/>
		<property name="openId" length="100" column="OPEN_ID"/>
		<property name="useDate" column="USE_DATE"/>
		<property name="createId" length="20" column = "CREATE_ID"/>	
		<property name="createTime" column = "CREATE_TIME"/>
		<property name="updateId" length="20" column = "UPDATE_ID"/>
		<property name="updateTime" column = "UPDATE_TIME"/>
		
	</class>
</hibernate-mapping>